#!/usr/bin/env groovy
pipeline{
agent any
parameters {
        string(name: 'a', defaultValue: 'master', description: 'Comapare which branch?')

        string(name: 'b', defaultValue: 'dev', description: 'Compare with which branch?')
    }
stages 
{
stage('Build') 
{
steps{
    sh 'rm -r jenkins'
    sh 'git clone https://github.com/DialgicMew/jenkins.git'
    sh 'ls'
    sh 'cd jenkins'
    echo "Comparing Branches"
    sh "git diff remotes/origin/${params.a} remotes/origin/${params.b}"  
}
}
stage('Test') 
{
steps{
echo "Testing"
}
}
stage('Deploy') 
{
steps{
echo "Deploying"
}
}
}
}