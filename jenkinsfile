#!/usr/bin/env groovy
pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
 echo "Building the Project.........."
 sh 'git branch -a'
 sh 'git diff remotes/origin/master remotes/origin/dev'
}
}
stage('Test') 
{
steps{
echo "Testing the Project.........."
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}