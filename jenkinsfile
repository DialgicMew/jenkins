{
stage('Build') 
{
steps{
 echo "Building the Project.........."
 git 'git clone  https://github.com/DialgicMew/jenkins.git'
 
}
}
stage('Test') 
{
steps{
echo "Testing the Project.........."
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}