#!/usr/bin/env groovy
pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
 echo "Building the Project.........."
//  powershell '''
//  $DiffToMaster = & git diff master dev
//  echo $DiffToMaster
//  '''
script{
def proc = "git diff master dev".execute()
def b = new StringBuffer()
proc.consumeProcessErrorStream(b)

println proc.text
println b.toString()
}
}
}
stage('Test') 
{
steps{
echo "Testing the Project.........."
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}